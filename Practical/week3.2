
#include <iostream>
using namespace std;
 

int main()
{
    

    int i,n, key, j;
    cout<<"enter size of array: ";
    cin>>n;
    int arr[n];
    int count=0;
    for(i=0; i<n; i++)
    {cout<<"enter the"<<" "<<i<<" "<<"element: ";
        cin>>arr[i];
    }
    for (i = 1; i < n; i++)
    {
        key = arr[i];
        j = i - 1;
        while (j >= 0 && arr[j] > key)
        {
            arr[j + 1] = arr[j];
            j = j - 1;
        }
        arr[j + 1] = key;
       - count++;
    }
    for (i = 0; i < n; i++)
    {
        cout << arr[i] << " ";
    cout << endl;
    }
    cout<<count<<endl;
    return 0;
}
