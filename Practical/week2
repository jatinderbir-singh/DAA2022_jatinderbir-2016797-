#include<iostream>
#include<stdlib.h>
using namespace std;
int main()
{
    
    int n;
    cout<<"ENTER THE SIZE OF ARRAY: "<<endl;
    cin >>n;
    int A[n];
    int key;
    cout<<"ENTER KEY TO FIND: "<<endl;
    cin>>key;
    int count=0;
    for(int i=0; i<n; i++)
    {    
        cin>>A[i];
        if(A[i]==key)
        count++;
    }
        cout<<"no. of copies of key: "<<count<<endl;
}
