#include<iostream>
using namespace std;
int main()
{
    int count=0;
    int n;
    cout<<"enter size of array: ";
    cin>>n;
    int a[n];
    int min_ind;
   
     for(int i=0; i<n; i++)
     {
        cin>>a[i];
    }
    for(int i=0; i<n-1; i++)
    {
        min_ind=i;
        for(int j=i+1;j<n; j++)
        {
    
        if(a[min_ind]> a[j])
        {     min_ind=j;
            int temp=a[min_ind];
            a[min_ind]=a[i];
            a[i]=temp;
        }
        count++;
        }
        
    }
    for(int i=0; i<n; i++)
     {
        cout<<a[i]<<" ";
        cout<<endl;
    }
      
    cout<<count<<endl;
return 0;    
}
