
#include <iostream>
using namespace std;
void printcommon(int arr1[],int len1,int arr2[],int len2)
{
int i=0,j=0;
    while(len1 > i && len2  >j)
    {
        if(arr1[i] < arr2[j])
        {
            i++;
        }
        else if(arr2[j] < arr1[i])
        {
            j++;
        }
        else
        {
          cout<<"common element is "<<arr1[i]<<endl;
          i++;
          j++;
        }
    }
}    
int main()
{
    int n,m;
    int arr1[n];
    int arr2[m];
    cout<<"enter length of array 1: ";
    cin>>n;
    for(int i=0;i<n;i++)
    {
        cout<<"enter elements of array 1: ";
        cin>>arr1[i];
    }
    cout<<"enter length of array 2: ";
    cin>>m;
    for(int i=0;i<m;i++)
    {
        cout<<"enter elements of array 2: ";
        cin>>arr2[i];
    }
    int len1=n;
    int len2=m;
 printcommon(arr1,len1,arr2,len2);
}
